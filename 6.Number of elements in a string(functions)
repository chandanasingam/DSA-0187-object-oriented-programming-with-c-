#include <iostream>
#include <string>
using namespace std;

int countElements(const string& str) {
    return str.length(); 
}

int main() {
    string myString;
    
    cout << "Enter a string: ";
    getline(cin, myString); // Allowing spaces in the string
    
    int numElements = countElements(myString);
    
    cout << "The number of elements in the string is: " << numElements << endl;
    
    return 0;
}
